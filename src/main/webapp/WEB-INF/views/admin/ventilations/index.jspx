<div id="listLogs" xmlns:c="http://java.sun.com/jsp/jstl/core" xmlns:spring="http://www.springframework.org/tags" xmlns:util="urn:jsptagdir:/WEB-INF/tags/util" xmlns:jsp="http://java.sun.com/JSP/Page" version="2.0">
  <jsp:directive.page contentType="text/html;charset=UTF-8" />
  <jsp:output omit-xml-declaration="yes" />
  <spring:message var="app_name" code="application_name" htmlEscape="false" />
  <spring:message var="title" code="welcome_titlepane" arguments="${app_name}" htmlEscape="false" />
  <spring:url value="/resources/images/csv_text.png" var="csvImgUrl" />
  <spring:url value="/admin/csv" var="csvUrl" />
  <a href="${csvUrl}" data-role="button" class="absButton"><img src="${csvImgUrl}" alt="CSV"/></a>
  <util:panel id="title" title="${title}">
		<div class="row">
			<div class="col-lg-12">
				<c:forEach items="${ventilations}" var="ventilation">
					<h2>${ventilation.date}</h2>
					<h3>Remise</h3>
					<dl>
						<dt>Remise banque (centimes)</dt>
						<dd>${ventilation.remise.montant}</dd>
						<dt>Nombre de transactions</dt>
						<dd>${ventilation.remise.nbTransactions}</dd>
					</dl>
					<h3>Transactions</h3>
					<dl>
						<dt>Total (centimes)</dt>
						<dd>${ventilation.totalMontantTransactions}</dd>
						<dt>Nombre de transactions</dt>
						<dd>${ventilation.transactions.size()}</dd>
					</dl>
				</c:forEach>
			</div>
		</div>
	</util:panel>
</div>